<%=form_with model: @audition do |f|%>
    <%= actor_id_field(@audition)%> <br>
    <%= project_id_field(@audition)%>
    
    <div class="form-group">
        <%= f.label :location, "Location:" %>
        <%= f.text_field :location %>
    </div>

    <div class="form-group">
        <%= f.label :time, "Time:" %>
        <%= f.text_field :time %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>
